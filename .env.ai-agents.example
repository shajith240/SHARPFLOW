# AI Agents Environment Variables
# Copy this file to .env and fill in your actual API keys

# OpenAI API Key (required for Prism, Sage, and Sentinel agents)
OPENAI_API_KEY=your_openai_api_key_here

# Apollo.io API Key (required for Falcon agent lead generation)
APOLLO_API_KEY=your_apollo_api_key_here

# Apify API Key (required for Sage agent LinkedIn scraping and Trustpilot reviews)
APIFY_API_KEY=your_apify_api_key_here

# Perplexity API Key (required for Sage agent company research)
PERPLEXITY_API_KEY=your_perplexity_api_key_here

# Redis Configuration (required for job queue system)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password_if_needed

# JWT Secret for WebSocket authentication
JWT_SECRET=your_jwt_secret_for_websocket_auth

# Frontend URL for CORS (production)
FRONTEND_URL=https://your-domain.com

# Supabase Configuration (should already be set)
SUPABASE_URL=your_supabase_url
SUPABASE_ANON_KEY=your_supabase_anon_key

# Node Environment
NODE_ENV=development
